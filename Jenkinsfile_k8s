parallel(
  failFast: false,
  'maven-jdk8-nanoserver-test-semver': {
     buildDockerAndPublishImage('inbound-agent-maven:jdk8-nanoserver-test-semver', [
      dockerfile: 'maven/jdk8/Dockerfile.nanoserver',
      agentLabels: 'windows',
      platform: 'windows/amd64',
      useContainer: false,
      automaticSemanticVersioning: true,
    ])
  },
  'updatecli': {
    updatecli(action: 'diff')
    if (env.BRANCH_IS_PRIMARY) {
      updatecli(action: 'apply', cronTriggerExpression: '@weekly')
    }
  },
)
